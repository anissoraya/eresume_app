<% @videos.each do |video| %>
  <div align="right">
    <%= link_to edit_video_galleries_path(video), class: "btn btn-success" do %>
      <i class="glyphicon glyphicon-pencil" style="color:white"></i>
    <% end %>
    <%= link_to video, method: :delete, data: {confirm: 'Are you sure to delete?'}, class: "btn btn-danger" do %>
      <i class="glyphicon glyphicon-remove" style="color:white"></i>
    <% end %>
  </div>
  <div class="col-sm-4">
    <%= video.link %><br>
    <%= video.name %>
  </div>
<%end%>
